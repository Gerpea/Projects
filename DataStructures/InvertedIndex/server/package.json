{
  "name": "inverted-index-server",
  "version": "0.0.1",
  "description": "An Inverted Index is a data structure used to create full text search. Given a set of text files, implement a program to create an inverted index. Also create a user interface to do a search using that inverted index which returns a list of files that contain the query term / terms. The search index can be in memory.",
  "scripts": {
    "start": "npm-run-all --parallel watch server",
    "server": "nodemon dist/app.js --watch dist",
    "watch": "webpack --progress --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js"
  },
  "author": "gerpea",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "franc": "^5.0.0",
    "franc-min": "^5.0.0",
    "mongoose": "^5.11.12",
    "natural": "^2.4.5",
    "stopword": "^1.0.6",
    "uuid": "^8.3.2",
    "ws": "^7.4.2"
  }
}
